<body id="login_page">
<div class="bg">
	<div>
		<img src="images/bj.jpg" style="width:100%;height:100%;color: #FFFFFF">
	</div>
</div>
<div class="header">
	<div class="header_1">
		<div id="logo1" style="margin:3% 0% 0% 0%" align="center">
			<img src="images/name.png">
		</div>
		<div class="denglu" style="margin:0% 0% 0% -30%;border:1px solid #522124;">
			<div class="denglu_1" style="background-color: #f9f1f1">
				<span class="biaoti" style="color:#522124">后台登录</span>
				<div>
					<img src="images/log-xian.png" style="width:50%;height:10%;margin: 15px 0 0 0;">
				</div>
				<form class="cmxform" id="commentForm" method="get">
					<p style="margin-top:24px">
						<label for="cname" style="color:#522124">用户名:</label>
						<input id="cname" name="name" type="text" required style="width:226px;height:28px;">
					</p>
					<p style="margin-top:18px">
						<label for="cemail" style="color:#522124">密&nbsp;码:</label>
						<input id="cemail" type="password" required style="width:226px;height:28px;">
					</p>
					<p style="margin-top:30px">
						<input type="text" value="登录" id="dl" style="background-color: #522124;text-align: center;" readonly>
					</p>
				</form>
			</div>
		</div>
	</div>
</div>
