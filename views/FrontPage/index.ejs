      "<img src=\"images/banener.jpg\"/>",
      "<img src=\"images/lunbo1.jpg\"/>",
      "<img src=\"images/lunbo2.jpg\"/>",];
    runimg.info("#box");
    runimg.action("#box");
  }
</script>
