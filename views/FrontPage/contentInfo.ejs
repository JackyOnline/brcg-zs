<body>
<% include /FrontPage/q_header %>
<div id="contentInfo_page">
    <div style="width:1000px;height:25px;z-index:100;background-color:white;margin:15px auto 0;">
        <!--<p style="color:#0c73cf;font-weight: 800;">首页 > 机关党建 > 主题活动 > 正文</p>-->
        <input type="text" id="seq_no" value="<%=seq_no%>" hidden>
    </div>
    <div id='articleInfo' style="width:1000px;min-height:250px;background-color:#F9F9F9;margin:0 auto;padding: 20px">
        <h1 id="title" style="text-align: center"></h1>
        <br>
        <p id="docInfo" style="font-size: 10px;text-align: center"></p>
        <br>
        <hr color="#DADADA" style="width:80%;margin:0 auto;">
        <br>
        <div id="content" style="width:800px;min-height:250px;line-height:2;margin:0 auto;">
        </div>
    </div>
    <div style="width:1000px;min-height:100px;margin:0 auto;padding: 20px;margin-bottom: 130px;">
        <h3 style="color: #56abee">相关阅读</h3>
        <hr size="1" color="#e8e8e8"/>
        <hr size="1" width="80px" color="#56abee" style="text-align: left; margin-top: -3px;">
        <br>
        <div id="relation">
            <!--<p style="font-weight:bold">&emsp;■&emsp;&emsp;智创网络公司员工小陈</p>-->
        </div>
    </div>
</div>
<% include /FrontPage/q_footer %>
</body>
</html>
<script type="text/javascript">

